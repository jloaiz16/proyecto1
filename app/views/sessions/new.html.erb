<div style="width: 70%; margin:0 auto; ">
<h1>Inicio de sesion</h1>
	<%= form_tag '/login' do %>
		Email: <br> <%= text_field_tag :email%> <br>
 		Contraseña: <br> <%= password_field_tag :password %> <br>
		 <%= submit_tag "Enviar" %>
	<% end %>
	¿No tienes una cuenta? <%= link_to 'Registrate', '/signup' %>
</div>
